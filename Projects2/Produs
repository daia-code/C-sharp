using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace L3
{
    public class Produs : ProdusAbstract
    {
        private long Id;
        private String Nume;
        private String CodIntern;
        private String Producator;
        public Produs(long Id, String Nume,String unCodIntern, String producator): base(Id, Nume, unCodIntern)
        {
            this.Producator = producator;
        }
        public override String Descriere()
        {
            return "Produsul [" + Id + "]: " + Nume + " cod intern :[" + CodIntern + "] "+ " Producator: [" + Producator + "]";
        }
        public virtual String AltaDescriere()
        {
            return "[" + Id + "]: " + Nume + " cod intern :[" + CodIntern + "] " + " Producator: [" + Producator + "]";
        }





    }
}
